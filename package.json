{
  "name": "@cityssm/puppeteer-launch",
  "version": "0.2.1",
  "description": "Launch Puppeteer, falling back to system browsers when the cached ones aren't working or aren't available.",
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "cross-env DEBUG=puppeteer-launch mocha --timeout 120000 --exit",
    "coverage": "cross-env DEBUG=puppeteer-launch c8 --reporter=lcov --reporter=text --reporter=text-summary mocha --timeout 120000 --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cityssm/puppeteer-launch.git"
  },
  "keywords": [
    "chrome",
    "chromium",
    "firefox",
    "puppeteer",
    "web-browser"
  ],
  "author": "The Corporation of the City of Sault Ste. Marie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cityssm/puppeteer-launch/issues"
  },
  "homepage": "https://github.com/cityssm/puppeteer-launch#readme",
  "dependencies": {
    "@cityssm/web-browser-info": "^1.0.0",
    "debug": "^4.3.4",
    "puppeteer": "^22.7.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/mocha": "^10.0.6",
    "cross-env": "^7.0.3",
    "eslint-config-cityssm": "^2.0.1"
  }
}

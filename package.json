  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cityssm/puppeteer-launch/issues"
  },
  "homepage": "https://github.com/cityssm/puppeteer-launch#readme",
  "dependencies": {
    "@cityssm/web-browser-info": "^1.0.0",
    "debug": "^4.3.6",
    "puppeteer": "^23.1.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "cross-env": "^7.0.3",
    "eslint-config-cityssm": "^6.0.0",
    "prettier-config-cityssm": "^1.0.0"
  }

{
  "name": "@cityssm/puppeteer-launch",
  "version": "3.0.0",
  "description": "Launch Puppeteer, falling back to system browsers when the cached ones aren't working or aren't available.",
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "cross-env DEBUG=puppeteer-launch:* node --test",
    "coverage": "cross-env DEBUG=puppeteer-launch:* c8 --reporter=lcov --reporter=text --reporter=text-summary node --test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cityssm/puppeteer-launch.git"
  },
